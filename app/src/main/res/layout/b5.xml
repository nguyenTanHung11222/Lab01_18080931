<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#75A4ECA7">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="REGISTER" />

    <EditText
        android:id="@+id/editTextTextEmailAddress4"
        android:layout_width="333dp"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="112dp"
        android:layout_marginEnd="5dp"
        android:background="#198F8E8E"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:shadowColor="#FFFFFF"
        android:text="      Phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/editTextTextEmailAddress4"
        android:layout_width="323dp"
        android:layout_height="43dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="5dp"
        android:background="#198F8E8E"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:shadowColor="#FFFFFF"
        android:text="      Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/editTextTextEmailAddress4"
        android:layout_width="333dp"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="268dp"
        android:layout_marginEnd="5dp"
        android:background="#198F8E8E"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:shadowColor="#FFFFFF"
        android:text="      Password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/editTextTextEmailAddress4"
        android:layout_width="333dp"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="5dp"
        android:background="#198F8E8E"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:shadowColor="#FFFFFF"
        android:text="      Email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/editTextTextEmailAddress4"
        android:layout_width="333dp"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="352dp"
        android:layout_marginEnd="5dp"
        android:background="#198F8E8E"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:shadowColor="#FFFFFF"
        android:text="      Birthday"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.573"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <RadioButton
        android:id="@+id/radioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="Male"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="544dp" />

    <RadioButton
        android:id="@+id/radioButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="Female"
        app:layout_constraintStart_toEndOf="@+id/radioButton"
        tools:layout_editor_absoluteY="544dp" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="208dp"
        android:layout_height="78dp"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress4"
        tools:layout_editor_absoluteX="37dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="333dp"
        android:layout_height="47dp"
        android:layout_marginTop="20dp"
        android:text="REGISTER"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        tools:text="When you agree to terms and conditions" />


</androidx.constraintlayout.widget.ConstraintLayout>